{
    "name": "what-to-cook",
    "version": "1.0.0",
    "description": "Helps decide what to cook",
    "main": "index.js",
    "private": true,
    "scripts": {
        "start": "node index.js",
        "dev": "concurrently \"cd client && npm run start\" \"cross-env NODE_ENV=development nodemon index.js --ignore ./client\"",
        "heroku-postbuild": "npm install --save-exact && cd client && npm install --only=dev --save-exact && npm install --save-exact && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jrga03/what-to-cook.git"
    },
    "author": "Jason Acido",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jrga03/what-to-cook/issues"
    },
    "homepage": "https://what-to-cook-app.herokuapp.com/",
    "dependencies": {
        "connect-mongo": "2.0.3",
        "draft-js-alignment-plugin": "2.0.5",
        "draft-js-drag-n-drop-plugin": "2.0.3",
        "draft-js-focus-plugin": "2.2.0",
        "draft-js-image-plugin": "2.0.6",
        "draft-js-plugins-editor": "2.1.1",
        "draft-js-resizeable-plugin": "2.0.8",
        "express": "4.16.4",
        "express-graphql": "0.7.1",
        "express-session": "1.15.6",
        "graphql": "14.1.1",
        "mongoose": "5.4.19",
        "react-device-detect": "1.6.2"
    },
    "devDependencies": {
        "cross-env": "5.2.0",
        "dotenv": "8.0.0"
    },
    "engines": {
        "node": "10.16.0",
        "npm": "6.9.0"
    }
}
